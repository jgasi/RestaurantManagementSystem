<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!--Background Color -->
    <Color x:Key="primaryBackColor1">#E5D3B3</Color>
    <Color x:Key="primaryBackColor2">#D2B48C</Color>
    <Color x:Key="secondaryBackColor1">#B99976</Color>

    <!--Window border Color -->
    <Color x:Key="windowBorderColor1">#987554</Color>
    <Color x:Key="windowBorderColor2">#664229</Color>

    <!--Color Palette (uzmi si koje hoces tu kasnije vise njih) -->
    <SolidColorBrush x:Key="color1">White</SolidColorBrush>
    <SolidColorBrush x:Key="color2">Black</SolidColorBrush>

    <!--Font Color -->
    <SolidColorBrush x:Key="titleColor1">White</SolidColorBrush>
    <SolidColorBrush x:Key="titleColor2">Black</SolidColorBrush>

    <!--Panel Color -->
    <SolidColorBrush x:Key="panelColor1">Black</SolidColorBrush>
    <SolidColorBrush x:Key="PanelOverColor">White</SolidColorBrush>
    <SolidColorBrush x:Key="panelActiveColor">Beige</SolidColorBrush>

    <!--Button Color -->
    
    
    <!--TextBox Color -->

</ResourceDictionary>